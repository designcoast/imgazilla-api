{
  "name": "imgazilla-api",
  "version": "0.0.1",
  "description": "API for imgazilla Figma plugin",
  "author": "imgazilla team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run",
    "migration:rollback": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate -- -d src/configs/sources/data-source.ts src/database/migrations/$npm_config_name",
    "migration:create": "npm run typeorm migration:create src/database/migrations/$npm_config_name"
  },
  "dependencies": {
    "@grammyjs/nestjs": "^0.3.4",
    "@lemonsqueezy/lemonsqueezy.js": "^3.2.0",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/throttler": "^6.1.1",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^8.25.0",
    "@sentry/profiling-node": "^8.25.0",
    "@sentry/tracing": "^7.114.0",
    "archiver": "^7.0.1",
    "archiver-zip-encrypted": "^2.0.0",
    "base64-arraybuffer": "^1.0.2",
    "bull": "^4.16.0",
    "bullmq": "^5.12.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "grammy": "^1.28.0",
    "helmet": "^7.1.0",
    "mongoose": "^8.5.3",
    "nestjs-grammy": "^0.0.3",
    "pako": "^2.1.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "potrace": "^2.1.8",
    "raw-body": "^3.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "svgo": "^3.3.2",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.4.1",
    "@types/archiver": "^6.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.2.0",
    "@types/pdfkit": "^0.13.4",
    "@types/potrace": "^2.1.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
